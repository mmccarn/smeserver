max_execution_time                     = {
    my $et = $php70{MaxExecutionTime} || $php{MaxExecutionTime} || "30";
    $OUT .= "$et";
}
memory_limit                           = {
    my $ml = $php70{MemoryLimit} || $php{MemoryLimit} || "128M";
    $OUT .= "$ml";
}
;added by stephdl see $php70{MaxInputTime}
max_input_time                     = {
    my $it = $php70{MaxInputTime} || $php{MaxInputTime} || "60";
    $OUT .= "$it";
}

